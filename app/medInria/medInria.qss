/**
 * QPushButton
 *
 */

QPushButton, QComboBox {
    border: 1px solid #6b6b6b;
    border-radius: 11px;
    background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #5e5e5e, stop: 1 #2b2b2b);
    color: #b2b8b2;
    font-size: 10px;
    height: 20px;
    padding-left: 5px;
    padding-right: 5px;
    min-width: 20px;
}

QPushButton:hover, QComboBox::hover {
    border: 1px solid #a7b6d3;
}

QPushButton:pressed, QPushButton:checked {
    border: 1px solid #ffffff;
    background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #282828, stop: 1 #cfcfcf);
}

QPushButton::menu-indicator, QComboBox::drop-down {
    image: url(:/pixmaps/drop-down.png);
    subcontrol-origin: padding;
    subcontrol-position: center right;

    right: 7px;
}

QPushButton:flat {
    border: none;
    background-color: none;
    color: #b2b8b2;
    height: 32px;
    font-size: 10px;
}

QPushButton:flat:hover {
    border: none;
}

QPushButton:flat:pressed {
    border: none;
    background-color: none;
}

QPushButton::menu-indicator:flat {
    image: none;
}

QComboBox QAbstractItemView {
    border: none;
    border-radius: 3px;

    background-color: #4b4b4b;
    color: #b2b8b2;

    selection-background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #6ea1f1, stop: 1 #567dbc);
    selection-color: #b2b8b2;

    font-size: 11pt;
}

/**
 * QSplitter
 *
 */

QSplitter::handle {
    image: url(:/pixmaps/splitter-handle.png);
}

QSplitter::handle:horizontal {
    width: 8px;

    background: qlineargradient(x1: 0, y1: 0, x2: 1, y2: 0, stop: 0 #5e5e5e, stop: 1 #2b2b2b);

border: 1px solid gray;
}

QSplitter::handle:vertical {
    height: 8px;

    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #5e5e5e, stop: 1 #2b2b2b);

border: 1px solid gray;
}

/**
 * QToolButton
 *
 */

QHelpSearchQueryWidget QToolButton {
    border: 1px solid #6b6b6b;
    border-radius: 2px;
    background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #5e5e5e, stop: 1 #2b2b2b);
    color: #b2b8b2;
    font-size: 10px;
    height: 20px;
    padding-left: 5px;
    padding-right: 5px;
    max-width: 20px;
    max-height: 20px;
}

QHelpSearchQueryWidget QToolButton:hover {
    border: 1px solid #a7b6d3;
}

QHelpSearchQueryWidget QToolButton:pressed {
    border: 1px solid #a7b6d3;
    background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #282828, stop: 1 #4a4a4a);
}

QHelpSearchQueryWidget QToolButton::menu-indicator {
    image: url(:/pixmaps/drop-down.png);
    subcontrol-origin: padding;
    subcontrol-position: center right;

    right: 7px;
}


/**
 * QGroupBox
 *
 */

QGroupBox {
    border: 2px solid gray;
    border-radius: 3px;
    margin-top: 1ex;

    color: #b2b8b2;
    font-size: 10pt;
}

QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top center;
    padding: 0 3px;

    color: #b2b8b2;
    font-size: 10pt;
}

QGroupBox::indicator::checked {
    image: url(:/pixmaps/checkbox-checked.png);
}

QGroupBox::indicator::unchecked {
    image: url(:/pixmaps/checkbox.png);
}

/**
 * QMenu
 *
 */

QMenu {
    border: transparent;
    border-radius: 5px;
    background-color: rgba(75, 75, 75, 128);
    color: #b2b8b2;
    font-size: 10pt;
}

QMenu::indicator {
     width: 15px;
     height: 15px;
 }

QMenu::indicator:non-exclusive:unchecked {
    image: url(:/pixmaps/checkbox.png);
}

QMenu::indicator:non-exclusive:checked {
    image: url(:/pixmaps/checkbox-checked.png);
}

QMenu::indicator:exclusive:unchecked {
    image: url(:/pixmaps/radio.png);
}

QMenu::indicator:exclusive:checked {
    image: url(:/pixmaps/radio-checked.png);
}

QMenu::separator {
    border: 1px solid #a9a9a9;
}

QMenu::item:hover {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #e7effd, stop: 1 #cbdaf1);
}

QMenu::item:selected:active {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #6ea1f1, stop: 1 #567dbc);
}

QMenu::item:selected:!active {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #6b9be8, stop: 1 #577fbf);
}

/**
 * QTabWidget & QTabBar

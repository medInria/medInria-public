    color: #b2b8b2;
}



medToolBoxTabBar::tab:selected, medToolBoxTabBar::tab:hover {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #505050, stop: 1.0 #5c5c5c);

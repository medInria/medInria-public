/**
* QToolTip
*/
QToolTip {
    color: #2a2a2a;
    background: #fff8dc;
    border: 1px solid #4b4b4b;
}

QToolTip:disabled {
    color: #2a2a2a;
}

/**
 * QTableView
 */

QTableView {
    background: #4b4b4b;
    border: 1px solid darkgray;
    padding: 0px 0px 0px 0px;
    gridline-color: gray;
}

/**
 * QListView
 *
 */

QListView {
    color: #CCCCCC;
    background: #6b6b6b;
    border: 1px solid darkgray;
    padding: 0px 0px 0px 0px;
    gridline-color: gray;

    alternate-background-color: #606060;
}

/**
 * QTreeView
 *
 */

QTreeView {
    color: #CCCCCC;
    background: #6b6b6b;
    border: 1px solid darkgray;
    padding: 0px 0px 0px 0px;
    gridline-color: gray;

    alternate-background-color: #606060;
}

/**
 * QTextEdit
 *
 */

QTextEdit {
    color: #CCCCCC;
    background: #6b6b6b;
    border: 1px solid darkgray;
    padding: 0px 0px 0px 0px;
}

/**
 *  QDateEdit
 */
QDateEdit{
    color:#CCCCCC;
    background: #6b6b6b;
    spacing: 5px;
}

/**
 * QSpinBox
 *
 */

QSpinBox{
    color:#CCCCCC;
    background: #6b6b6b;
    spacing: 5px;
}

/**
 * QDoubleSpinBox
 *
 */

QDoubleSpinBox{
    color:#CCCCCC;
    background: #6b6b6b;
    spacing: 5px;
}

/**
 * QCheckBox
 *
 */

QCheckBox,QTreeView::indicator  {
    color: #b2b8b2;
    font-size: 10px;
    spacing: 5px;
}

QCheckBox::indicator,QTreeView::indicator {
    width: 15px;
    height: 15px;
}

QCheckBox::indicator:unchecked {
    image: url(:/pixmaps/checkbox.png);
}

QCheckBox::indicator:checked {
    image: url(:/pixmaps/checkbox-checked.png);
}

QCheckBox::indicator:indeterminate {
    image: url(:/pixmaps/checkbox-partial.png);
}

QTreeView::indicator:indeterminate {
    image: url(:/pixmaps/checkbox-partial.png);
}


QTreeView::indicator:checked
{
     background: url(:/pixmaps/checkbox-checked.png);
     /*background: url(":checkbox/checked");*/
}

QTreeView::indicator:unchecked
{
    image: url(:/pixmaps/checkbox.png);
     /*background: url(":checkbox/unchecked");*/
}

/**
 * QLabel
 *
 */

QLabel {
    color: #b2b8b2;
    font-size: 10px;
}

QWidget:disabled {
    color: #303030;
}



/**
 * QTableWidget
 *
 */

QTableWidget {
    border: 1px solid #2a2a2a;
    color: #b2b8b2;
}

/**
 * QHeaderView
 *
 */

QHeaderView {
    border: 1px solid #2a2a2a;
    border-left: none;
    border-right: none;
    background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #535353, stop: 1 #454545);
    color: #b2b8b2;
    font-size: 10px;
    height: 22px;
}

QHeaderView::section {
    border: 1px solid #2a2a2a;
    border-top: none;
    border-bottom: none;
    border-left: none;
    background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #535353, stop: 1 #454545);
    color: #b2b8b2;
    font-size: 10px;
    height: 22px;
    padding-left: 5px;
}

/**
 * QLineEdit
 *
 */

QLineEdit {
    color: #b2b8b2;
    border: 1px solid gray;
    border-radius: 9px;
    padding: 0 8px;
    background: #4b4b4b;
    selection-background-color: #9eabc6;
}

/**
 * QProgressBar
 *
 */

QProgressBar {
    border: 1px solid #6b6b6b;
    border-radius: 8px;
    background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #5e5e5e, stop: 1 #2b2b2b);
    color: #b2b8b2;
    font-size: 10px;
    height: 14px;
    text-align: right;
    padding-left: 1px;
    padding-right: 25px;
}

QProgressBar::chunk {
    border: 1px solid #6b6b6b;
    border-radius: 6px;
    background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #a0b0ce, stop: 1 #466096);
    margin: 1px;
}

/**
 * QStatusBar
 *
 */

QStatusBar {
    background-image: url(:/pixmaps/statusbar.png);
    background-repeat: repeat-x;
    color: #b2b8b2;
    font-size: 10px;
    height: 31px;
}

QStatusBar::item {
    border: none;
}

/**
 * QRadioButton
 *
 */

QRadioButton {
    color: #b2b8b2;
    font-size: 10px;
    spacing: 5px;
}

/*QRadioButton:disabled {
    color: #000000;
}*/

QRadioButton::indicator {
    width: 15px;
    height: 15px;
}

QRadioButton::indicator:unchecked {
    image: url(:/pixmaps/radio.png);
}

QRadioButton::indicator:checked {
    image: url(:/pixmaps/radio-checked.png);
}

QRadioButton::indicator:indeterminate {
    image: url(:/pixmaps/radio-partial.png);
}

/**
 * QSlider
 *
 */

QSlider::groove:horizontal {
    border-image: url(:/pixmaps/slider-horizontal-groove.png) 0 6 0 6 repeat-x;
    border-left-width: 6px;
    border-right-width: 6px;
}

QSlider::handle:horizontal {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #9eabc6, stop:1 #383838);
    border: 1px solid #383838;
    border-radius: 3px;
    width: 6px;
}

QSlider::groove:vertical {
    border-image: url(:/pixmaps/slider-vertical-groove.png) 6 0 6 0 repeat-y;
    border-top-width: 6px;
    border-bottom-width: 6px;
}

QSlider::handle:vertical {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0, stop:0 #9eabc6, stop:1 #383838);
    border: 1px solid #383838;
    border-radius: 3px;
    height: 6px;
}

/**
 * medDatabaseView
 *
 */

medDatabaseView {
    alternate-background-color: #505050;

    border-image: url(:/pixmaps/treeview-frame.png) 16 16 16 16;
    border-top-width: 16px;
    border-left-width: 16px;
    border-right-width: 16px;
    border-bottom-width: 16px;

    padding-top: -7px;
    padding-left: -4px;
    padding-right: -5px;

    color: #b2b8b2;

    background: #313131;
}

medDatabaseView::branch:has-siblings:!adjoins-item {
    image: none;
}

medDatabaseView::branch:has-siblings:adjoins-item {
    image: none;
}

medDatabaseView::branch:!has-children:!has-siblings:adjoins-item {
    image: none;
}

medDatabaseView::branch:has-children:!has-siblings:closed,
medDatabaseView::branch:closed:has-children:has-siblings {
    border-image: none;
    image: url(:pixmaps/treeview-branch-closed.png);
}

medDatabaseView::branch:open:has-children:!has-siblings,
medDatabaseView::branch:open:has-children:has-siblings  {
    border-image: none;
    image: url(:pixmaps/treeview-branch-open.png);
}


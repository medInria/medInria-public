/**
 * QScrollBar
 *
 * pseudo-state: horizontal
 */

QScrollBar:horizontal {
    background-image: url(:/pixmaps/scrollbar-horizontal-background.png);
    background-repeat: repeat-x;
    border-left-color: #2a2a2a;
    border-left-style: solid;
    border-left-width: 1px;
    height: 16px;
    margin-top: 0px;
    margin-bottom: 0px;
    margin-left: 0px;
    margin-right: 34px;
}

QScrollBar::handle:horizontal {
    border-image: url(:/pixmaps/scrollbar-horizontal-handle.png) 0 6 0 6 repeat-x;
    border-left: 6px;
    border-right: 6px;
    height: 16px;
    min-width: 20px;
}

QScrollBar::add-line:horizontal {
    background-image: url(:/pixmaps/scrollbar-horizontal-addline.png);
    width: 17px;
    height: 16px;
    subcontrol-position: right;
    subcontrol-origin: margin;
}

QScrollBar::sub-line:horizontal {
    background-image: url(:/pixmaps/scrollbar-horizontal-subline.png);
    width: 17px;
    height: 16px;
    subcontrol-position: top right;
    subcontrol-origin: margin;
    position: absolute;
    right: 17px;
}

QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal {
    background: none;
}

/**
 * QScrollBar
 *
 * pseudo-state: vertical
 */

QScrollBar:vertical {
    background-image: url(:/pixmaps/scrollbar-vertical-background.png);
    background-repeat: repeat-y;
    border-left-color: #2a2a2a;
    border-left-style: solid;
    border-left-width: 1px;
    width: 16px;
    margin-top: 0px;
    margin-bottom: 34px;
    margin-left: 0px;
    margin-right: 0px;
}

QScrollBar::handle:vertical {
    border-image: url(:/pixmaps/scrollbar-vertical-handle.png) 6 0 6 0 repeat-y;
    border-top: 6px;
    border-bottom: 6px;
    height: 17px;
    min-height: 20px;
}

QScrollBar::add-line:vertical {
    background-image: url(:/pixmaps/scrollbar-vertical-addline.png);
    width: 16px;
    height: 17px;
    subcontrol-position: bottom;
    subcontrol-origin: margin;
}

QScrollBar::sub-line:vertical {
    background-image: url(:/pixmaps/scrollbar-vertical-subline.png);
    width: 16px;
    height: 17px;
    subcontrol-position: left bottom;
    subcontrol-origin: margin;
    position: absolute;
    bottom: 17px;
}

QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
    background: none;
}

